{{> doctype}}
<html>
<head>
	{{> headers}}
	<link rel="stylesheet" type="text/css" href="stylesheets/wishlist.css">
</head>
<body>
<script type="text/javascript" src="javascripts/facebook-api.js"></script>
	<div id="navigation-base">
		<div id="navigation-bar">
			<a href="search_page.html" class="pull-left" id="logo">
				<img src="images/icons/main_logo.png" alt=""  id="logo-img">
				<span id="logo-char">Locate Mate</span>
			</a>
			<span id="help" class="nav pull-right">
				<a class="link-reset" href="">Help</a>
			</span>
			<span class ="nav pull-right">
				<span id="user-name">Kate</span>
			</span>
			<span class="nav pull-right">
				<img src="https://s3.amazonaws.com/uifaces/faces/twitter/kfriedson/48.jpg" id="user-img" alt="">
			</span>
		</div>
	</div>
	<div id="wishlist-container">
		<div id="wishlist-content" class="row">
			{{#each wishData}}
				{{>each_wish}}
			{{/each}}
		</div>
	</div>
</body>
</html>
